package com.stack;

import java.util.Scanner;

public class StackTester {
	public static void main(String[] args) {
		try(Scanner sc = new Scanner(System.in)){
			
			int choice;
			
			do {
				System.out.print("Enter your choice - ");
				choice = sc.nextInt();
				
			}while(choice!=4);
		}
	}
}
