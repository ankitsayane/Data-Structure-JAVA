package com.tester;

import com.operations.LinkedList;
import com.operations.SingleLinkedList;

public class Tester {

	public static void main(String[] args) {
		LinkedList a = new SingleLinkedList();
		
		a.insertAtFront(2);
		a.insertAtFront(6);
		a.insertAtRear(10);
		a.insertAtFront(1);
		a.insertAtRear(20);
		a.show();
		a.removeFromFront();
		a.show();

	}

}
