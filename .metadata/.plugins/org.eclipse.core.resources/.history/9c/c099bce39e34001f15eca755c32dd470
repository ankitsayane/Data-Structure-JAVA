package com.bst;

public class BinaryTreeImpl {
	class Node{
		int data;
		Node left;
		Node right;
		public Node(int data) {
			this.data = data;
			left = right = null;
		}
	}
	
	Node root;
	public BinaryTreeImpl() {
		root = null;
	}
	
	public void add(int data) {
		Node newNode = new Node(data);
	}
}
