package com.operations;

public class QueueOperations implements Queue {
	
	int size = SIZE;
	int[] queue = new int[size];
	int front = -1;
	int rear = -1;

	@Override
	public void enQueue(int ele) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void deQueue() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void show() {
		for(int q : queue) {
			System.out.print(q+" ");
		}
		System.out.println();
		
	}

	@Override
	public boolean isEmpty() {
		if(rear == -1) {
			return true;
		}
		return false;
	}

	@Override
	public boolean isFull() {
		if(rear == size-1) {
			return true;
		}
		return false;
	}

}
